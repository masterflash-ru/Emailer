{
    "name": "masterflash-ru/emailer",
    "description": "Генерация и рассылка HTML писем",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "masterflash-ru/ado": "^2.0",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-mail": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Emailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Emailer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev",
            "dev-develop": "0.0.1-dev"
        },
        "zf": {
            "component": "Mf\\Catalog",
            "config-provider": "masterflash-ru\\Mf\\Emailer\\ConfigProvider"
        }
    }
}
